{
    "expo": {
        "name": "fashion",
        "slug": "fashion",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#9AC4F8"
        },
            "updates": {
            "fallbackToCacheTimeout": 0
        },
            "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "config": {
                "googleMobileAdsAppId": "ca-app-pub-3484834519910203~6742757387"
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#FFFFFF"
            },
            "config": {
                "googleMobileAdsAppId": "ca-app-pub-3484834519910203~6742757387"
            }
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "plugins": [
            [
                "expo-image-picker",
                {
                    "photosPermission": "O app está pedindo permissão para acessar sua galeria de fotos."
                },
                "expo-ads-admob",
                {
                "userTrackingPermission": "This identifier will be used to deliver personalized ads to you."
                }
            ]
        ]
    }
}
